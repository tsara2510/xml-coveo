{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "/api/xml-file",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml; charset=iso-8859-1"
        }
      ]
    }
  ]
}
